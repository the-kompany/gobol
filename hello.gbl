OPEN("test.csv") AS file_handle

RECORD MYTHING
   DATE
   Title2 
   Title3 
END-RECORD

PERFORM READ file_handle INTO MYTHING AS CSV
 
  DISPLAY "hello"

  IF MYTHING.Title2 = "two" THEN 

  MOVE FMTDATE(MYTHING.DATE, "mm-dd-yyyy", "mm/dd/yy") TO MYTHING.DATE

  END-IF


  WRITE MYTHING TO "output.csv" as CSV 


END-PERFORM 

CLOSE file_handle


